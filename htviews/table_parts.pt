<metal:parts>
    <metal:macro define-macro="listing_large">
	<div id="browse-listings">
	<div class="null listing-wrapper prototype">
            <table>
		<tbody>
                    <tr>
			<td class="${slug}-view-link">
			    <div class="m-1 centered text-centered">
				<a class="small-button">View</a>
			    </div>
			    <div class="centered text-centered">
				<div><img src="" class="listing-avatar" /></div>
				<div><span class="listing-owner"></span></div>
			    </div>
			</td>
			<td>
			    <strong>&nbsp;Items:</strong>
			    <metal:wrapper>
				<metal:display use-macro="context.organizer_parts.macros.item_display" />
			    </metal:wrapper>
			</td>
		    </tr>
	            <tr>
			<td colspan="2">
			    <strong>Description:</strong>
			    <span class="listing-description"></span>
			</td>
                    </tr>

		</tbody>
            </table>
	</div>
	</div>
    </metal:macro>
</metal:parts>
