<metal:parts>


<!--

slim chooser:  default 2 rows * 5 cols

-->
<metal:macro
     metal:define-macro="chooser_slim"
     tal:define="slug slug | '';
		 rows rows | 2;
		 cols cols | 5;
		 ordered ordered | False; ">
    <div class="bp bp-slim" id="bp-chooser-${slug}">
	<div><span class="help"></span></div>
	<table class="bp-chooser center">
	    <caption>
		<div class="message null">
		    <div class="error" id="bp-chooser-${slug}-error"></div>
		</div>
 		<div class="message null">
		    <div class="warning" id="bp-chooser-${slug}-warn"></div>
		</div>
	    </caption>
	    <tbody>
		<metal:loop tal:repeat="row range(rows)">
		    <tr>
			<metal:loop tal:repeat="col range(cols)">
			    <td id="${slug}-${(row*cols)+col+1}"><div></div></td>
			</metal:loop>
		    </tr>
		</metal:loop>
	    </tbody>
	</table>
    </div>
</metal:macro>




<!--

thin backpack: dynamic rows, cols, and pages

-->
<metal:macro
     metal:define-macro="backpack_thin"
     tal:define="slug slug | '';
		 item_count item_count | 277;
		 rows rows | 4;
		 cols cols | 4; ">
    <div class="bp bp-thin" id="bp-${slug}">
	<div><h3></h3></div>
	<div class="bp-view">
	    <div class="bp-ordered" id="bp-ordered-${slug}">
		<div class="bp-pages center">
		    <div><span class="help"></span></div>
		    <metal:dynamic tal:define="pages range(1+(item_count / (rows*cols)));
					       rows range(rows);
					       cols range(cols);
					       cell_counter itertools.count(1)">
			<metal:loop tal:repeat="page pages">
			    <table class="bp-ordered bp-${page+1} bp-page-${slug} ${page and 'null' or ''}">
				<tbody>
				    <tr tal:repeat="row rows">
					<td tal:repeat="col cols" id="${slug}${cell_counter.next()}">
					    <div></div>
					</td>
				    </tr>
				</tbody>
			    </table>
			</metal:loop>
		    </metal:dynamic>
		    <metal:nav metal:use-macro="macros.backpack_navigator" />
		</div>
	    </div>
	</div>
    </div>
</metal:macro>




