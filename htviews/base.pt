<html xmlns="http://www.w3.org/1999/xhtml" lang="en" xml:lang="en"
      metal:define-macro="layout">
    <head metal:define-slot="head">
	<metal:slot define-slot="title" />
	<metal:slot define-slot="meta" />
	<metal:slot define-slot="style" />
	<metal:slot define-slot="script" />
	<metal:slot define-slot="feed" />
    </head>
    <body metal:define-slot="body">
	<div metal:define-slot="header" />
	<div metal:define-slot="main" />
	<div metal:define-slot="footer" />
    </body>
</html>
