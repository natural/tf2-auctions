<metal:organizers>


<div metal:define-macro="item_display">
    <table class="chooser">
        <tbody>
	    <metal:define tal:define="rows rows | 1; cols cols | 10">
		<metal:loop tal:repeat="row range(rows)">
    		    <tr>
			<metal:loop tal:repeat="col range(cols)">
			    <td class="item-display"><div class="defindex-lazy"></div></td>
			</metal:loop>
		    </tr>
		</metal:loop>
	    </metal:define>
	</tbody>
    </table>
</div>


<div metal:define-macro="item_chooser" id="chooser-${slug}">
    <table class="centered chooser">
	<caption><div class="message null"><div class="error" id="chooser-${slug}-error"></div></div></caption>
        <tbody>
	    <metal:define tal:define="rows rows | 2; cols cols | 5">
	    <metal:loop tal:repeat="row range(rows)">
    		<tr>
		    <metal:loop tal:repeat="col range(cols)">
			<td id="${slug}-${(row*cols)+col+1}"><div></div></td>
		    </metal:loop>
		</tr>
	    </metal:loop>
	    </metal:define>
	</tbody>
    </table>
</div>


