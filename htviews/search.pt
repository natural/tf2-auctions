<html metal:use-macro="context.content.macros.layout">
<head>
    <title metal:fill-slot="title">TF2Bay.com -- Search Listings</title>
    <metal:css fill-slot="style">
	<link href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/base/jquery-ui.css"
	      rel="stylesheet" type="text/css" />
        <metal:slot define-slot="style" />
    </metal:css>
    <metal:js fill-slot="script">
	<script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.5/jquery-ui.min.js"
		type="text/javascript"></script>
        <metal:slot define-slot="script" />
    </metal:js>
</head>
<body>
<metal:fill fill-slot="main" tal:define="slug 'search'">

    <h1><span class="hilite"></span>Search Listings</h1>

    <div id="${slug}-pod">

	<div id="${slug}-controls" class="null">
	    <div class="organizer-view" id="${slug}-controls-nav">
		<h2>Page:</h2>
		<div class="tools" id="${slug}-nav">
		    <div class="navs">
			<span class="nonav" id="${slug}-prev-none">&lt;</span>
			<span class="nav" id="${slug}-prev-link"><a href="" id="${slug}-prev">&lt;</a></span>
			<span class="nav" id="${slug}-next-link"><a href="" id="${slug}-next">&gt;</a></span>
			<span class="nonav" id="${slug}-next-none">&gt;</span>
		    </div>
		</div>
	    </div>

	    <div id="${slug}-basic" class="null">
		<div>
		    <a id="${slug}-basic-link" href="" onclick="return false">
			<h2>Basic <span class="hi">&lt;&lt;</span></h2>
		    </a>
		</div>
	    </div>

	    <div class="null" id="${slug}-advanced-pod">
		<div class="organizer-view">
		    <metal:include
			 tal:define="slug 'ac'"
			 metal:use-macro="context.organizer_parts.macros.item_backpack_narrow" />
		    <label>Your Search:</label>
		    <metal:include
			 tal:define="slug 'advanced-search'; rows 1; cols 3"
			 metal:use-macro="context.organizer_parts.macros.item_chooser" />
		</div>
	    </div>
            <div id="${slug}-filters"><h2>Filters:</h2> <div id="${slug}-filter-inputs"></div></div>
	    <div id="${slug}-sorts"><h2>Sort:</h2> <div id="${slug}-sort-inputs"></div></div>
	    <div id="${slug}-advanced">
		<div>
		    <a id="${slug}-advanced-link" href="" onclick="return false">
			<h2>Advanced <span class="hi">&gt;&gt;</span></h2>
		    </a>
		</div>
	    </div>
	</div>

	<div id="${slug}-listing-pod">
	    <div>
		<h2 class="null" id="${slug}-no-listings"></h2>
		<h2 class="null" id="${slug}-some-listings"></h2>
		    <div class="organizer-view" id="${slug}-results-pod">
			<metal:include
			     tal:define="slug 'search'; rows 2; cols 5;"
			     metal:use-macro="context.table_parts.macros.listings_large" />
		    </div>
	    </div>
	</div>

    </div>

</metal:fill>
</body>
</html>
