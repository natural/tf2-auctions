<metal:parts>
    <metal:macro define-macro="listing_large">
	<div id="browse-listings">
	<div class="null prototype">
            <table class="listing-table w100">
		<tbody>
                    <tr>
			<td class="${slug}-view-link" width="10%">
			    <div class="centered text-centered ${slug}-view-summary">
				<a class="small-button">View</a>
			    </div>
			</td>
			<td width="90%">
			    <strong>&nbsp;Items:</strong>
			    <metal:wrapper>
				<metal:display use-macro="context.organizer_parts.macros.item_display" />
			    </metal:wrapper>
			    <table class="w100">
				<tr>
				    <td class="w50"><strong>Description:</strong></td>
				    <td><strong>Created By:</strong></td>
				</tr>
				<tr>
				    <td><div class="listing-description"></div></td>
				    <td>
					<div class="">
					    <img src="" class="listing-avatar" />
					    <span class="listing-owner"></span>
					</div>
				    </td>
				</tr>
			    </table>
			</td>
		    </tr>
		</tbody>
            </table>
	</div>
	</div>
    </metal:macro>

    <metal:macro define-macro="listing_bids">
	<div class="null prototype">
            <table class="bid-table w100">
		<tbody>
                    <tr>
			<td class="${slug}-view-link" rowspan="2">
			    <div class="centered text-centered ${slug}-view-summary">
				<div style="white-space:nowrap;">
				    <img src="" class="bid-avatar" />
				    <span class="bid-owner"></span>
				</div>
			    </div>
			</td>
			<td class="w88">
			    <strong>&nbsp;Bid Items:</strong>
			    <metal:wrapper>
				<metal:display use-macro="context.organizer_parts.macros.item_display" />
			    </metal:wrapper>
			</td>
		    </tr>
	            <tr>
			<td>
			    <strong>Message:</strong>
			    <div class="bid-message"></div>
			</td>
                    </tr>
		</tbody>
            </table>
	</div>
    </metal:macro>


</metal:parts>
